<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.exam"
          android:versionCode="1"
          android:versionName="1.0">
    <uses-sdk android:minSdkVersion="15"/>
    <application android:label="@string/app_name"
                 android:icon="@drawable/ic_launcher"
                 android:theme="@android:style/Theme.Holo.Light">
        <activity android:name="MainActivity"
                  android:configChanges="keyboardHidden|orientation|screenSize"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="FirstRunActivity"
                  android:label="@string/set_name"
                  android:configChanges="keyboardHidden|orientation|screenSize"
                  android:windowSoftInputMode="adjustResize">
        </activity>
        <activity android:name="AddCarActivity"
                  android:label="@string/add_order"
                  android:configChanges="keyboardHidden|orientation|screenSize"
                  android:windowSoftInputMode="adjustResize">
        </activity>
        <activity android:name="EditCarActivity"
                  android:label="@string/edit_order"
                  android:configChanges="keyboardHidden|orientation|screenSize"
                  android:windowSoftInputMode="adjustResize">
        </activity>
    </application>
</manifest>
